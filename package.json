{
	"name": "vizard_web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"test": "jest --watch",
		"coverage": "jest --collect-coverage --passWithNoTests",
		"check-types": "tsc --pretty --noEmit",
		"check-format": "prettier --check .",
		"check-lint": "eslint . --ext ts --ext tsx --ext js",
		"format": "prettier --write .",
		"test-all": "yarn run check-format && yarn run check-lint && yarn run check-types && yarn run build",
		"prepare": "husky install"
	},
	"dependencies": {
		"next": "11.0.0",
		"react": "17.0.2",
		"react-dom": "17.0.2"
	},
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"@types/react": "17.0.11",
		"babel-jest": "^27.0.5",
		"eslint": "7.29.0",
		"eslint-config-next": "11.0.0",
		"eslint-config-prettier": "^8.3.0",
		"husky": "^6.0.0",
		"jest": "^27.0.5",
		"ts-jest": "^27.0.3",
		"ts-node": "^10.0.0",
		"typescript": "^4.3.4",
		"prettier": "^2.3.1"
	}
}
